#!/bin/bash
set -Eeuo pipefail
BINARY="${XDG_CACHE:-$HOME/.cache}/minikube/minikube"
if [[ ! -r "$BINARY" || ! -x "$BINARY" ]]
then
    mkdir -p "$(dirname "$BINARY")" > /dev/null
    curl -sSLo "$BINARY" https://storage.googleapis.com/minikube/releases/latest/minikube-linux-amd64
    chmod +x "$BINARY"
fi
exec "$BINARY" $*
