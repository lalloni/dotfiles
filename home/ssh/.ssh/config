ControlMaster auto
ControlPath /home/%u/.ssh/tmp_socket_%C
ControlPersist 300
HashKnownHosts no

Match exec="nmcli -t c show --active | grep -qE 'Nargothrond|Gondolin'" host=q10.com.ar
Hostname tulkas

Match !exec="nmcli -t c show --active | grep -qE 'Nargothrond|Gondolin'" host=tulkas
Hostname q10.com.ar

Match exec="grepcidr -f $HOME/.ssh/amazon-ip-ranges-ec2 <(echo %h)"
UserKnownHostsFile /dev/null
StrictHostKeyChecking no
